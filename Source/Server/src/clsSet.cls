VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
  Persistable = 0  'NotPersistable
  DataBindingBehavior = 0  'vbNone
  DataSourceBehavior  = 0  'vbNone
  MTSTransactionMode  = 0  'NotAnMTSObject
END
Attribute VB_Name = "clsObjectSet"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Explicit
'NO USADO
Private col As Collection


Private Sub Class_initialize()
    Set col = New Collection
End Sub

Public Sub Add(ByRef item As Object)
    col.Add item
End Sub

Public Sub Delete(ByRef item As Object)
    Dim i As Long
    i = FindIndex(item)
    If i > 0 And i <= col.Count Then
        col.Remove (i)
    End If
End Sub

Public Function GetCollection() As Collection
    Set GetCollection = col
End Function

Public Function Exists(ByRef item As Object) As Boolean
    If FindIndex(item) > 0 Then
        Exists = True
    End If
End Function

Private Function FindIndex(ByRef item As Object) As Long
    Dim i As Long
    For i = 1 To col.Count
        If col.item(i) = item Then
            FindIndex = i
            Exit Function
        End If
    Next
End Function
